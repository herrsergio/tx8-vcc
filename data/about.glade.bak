  <property name="logo">bender.png</property>
  <property name="copyright" translatable="yes">(C) 2007 Sergio Cuellar Valdes</property>
  <property name="comments" translatable="yes">tx8 Visual Command Center</property>
  <property name="authors">Sergio Cuellar Valdes
&lt;herrsergio@gmail.com&gt;</property>
  <property name="translator_credits" translatable="yes">translator_credits</property>
